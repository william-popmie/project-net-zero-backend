[codecarbon]

# --- General ---
country_iso_code = SWE
offline = false

# --- Avoid privileged power measurements (prevents password prompt) ---
allow_multiple_runs = true
tracking_mode = process
measure_power_secs = 15

# Disable low-level hardware / powermetrics style access (fixed wattage, no password prompt)
force_cpu_power = 15.0
force_ram_power = 3.0

# --- Output ---
save_to_file = true
output_dir = ./output-folder/codecarbon_output

# --- Logging ---
log_level = WARNING